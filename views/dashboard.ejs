            <div class="col-lg-12">
                <div class="card">
                    <div class="card-body">
                        <div style="text-align: center;padding-top:20px;">
                            <select onChange="ake()" id="sel">
                                <% schools.forEach(function(school) { %>
                                    <option value="<%= school %>"><%= school %></option>
                                <% }); %>

                            </select>

                        </div>
                    </div>
                </div>
            </div>
        </div>


        <div class="grid-container">
            <div class="row">
