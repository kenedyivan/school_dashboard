          <div class="row">
              <div class="col-md-6"> <div class="card">
                  <div class="card-body">
                      <h5 class="card-title">enrolment</h5>
                      <div class="maps">
                        <script>
                            
                            let boys = document.getElementById('boys').value;
                            let girls = document.getElementById('girls').value;
                            let school = document.getElementById('school').value;

                            function myFunction(num) {
                                 return parseInt(num, 10);
                            }

                            let boysPlot = JSON.parse(boys).map(myFunction);
                            let girlsPlot = JSON.parse(girls).map(myFunction);
                           

                        var barChartData = {
                            labels: [
                              "p1",
                              "p2",
                              "p3",
                              "p4",
                              "p5",
                              "p6",
                              "p7"
                            ],
                            datasets: [
                              {
                                label: "Boys",
                                backgroundColor: "pink",
                                borderColor: "red",
                                borderWidth: 1,
                                data: boysPlot
                              },
                              {
                                label: "Girls",
                                backgroundColor: "lightblue",
                                borderColor: "blue",
                                borderWidth: 1,
                                data: girlsPlot
                              }
                            ]
                          };
                          
                          var chartOptions = {
                            responsive: true,
                            legend: {
                              position: "top"
                            },
                            title: {
                              display: true,
                              text: school
                            },
                            scales: {
                              yAxes: [{
                                ticks: {
                                  beginAtZero: true
                                }
                              }]
                            }
                          }
                          
                          window.onload = function() {
                            var ctx = document.getElementById("mySuperChart").getContext("2d");
                            window.myBar = new Chart(ctx, {
                              type: "bar",
                              data: barChartData,
                              options: chartOptions
                            });
                          };  
                        </script>
                        <script src="https://unpkg.com/axios/dist/axios.min.js"></script>
                        <script>
                            // Make a request for a user with a given ID
                            axios.get('/enrollment-stats/delectus')
                            .then(function (response) {
                                // handle success
                                console.log(response.data);
                                return response.data;
                            })
                            .catch(function (error) {
                                // handle error
                                console.log(error);
                            })
                            .finally(function () {
                                // always executed
                            });
                        </script>



                        <canvas id="mySuperChart"></canvas>
                    </div>
                  </div>
                </div>
