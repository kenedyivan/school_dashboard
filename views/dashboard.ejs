                                <canvas id="attendance"></canvas>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <div class="row">
                <div class="col">
                    <div class="card">
                        <div class="card-body">
                            <h5 class="card-title">Teacher-pupil ratio</h5>
                            <div id="canvas-holder" style="width:60%">
                                <canvas id="chart-area" width="100" height="100;"></canvas>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col">
                    <div class="card">
                        <div class="card-body">
                            <h5 class="card-title">Teacher-pupil ratio</h5>
