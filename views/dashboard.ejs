                                        borderColor: "red",
                                        borderWidth: 1,
                                        data: [3, 5, 6, 7,3, 5, 6]
                                      },
                                      {
                                        label: "Girls Attendence",
                                        backgroundColor: "lightblue",
                                        borderColor: "blue",
                                        borderWidth: 1,
                                        data: [4, 7, 3, 6, 10,7,4]
                                      },
                                      
                                    ]
                                  };
                                  
                                  var chartOptionsData = {
                                    responsive: true,
                                    legend: {
                                      position: "top"
                                    },
                                    title: {
                                      display: true,
                                      text: "Chart.js Bar Chart"
                                    },
                                    scales: {
                                      yAxes: [{
                                        ticks: {
                                          beginAtZero: true
                                        }
                                      }]
                                    }
                                  }
                                  
                                  window.onload = function() {
                                    var ctx = document.querySelector("#myCanvas").getContext("2d");
                                    window.myBar = new Chart(ctx, {
                                      type: "line",
                                      data: barData,
                                      options: chartOptionsData
                                    });
                                  };
                        </script>
                        
                          <canvas id="myCanvas"></canvas>
                        </div>
